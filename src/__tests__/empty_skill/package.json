{
  "name": "empty-skill",
  "description": "Its pretty empty",
  "version": "0.0.1",
  "homepage": "https://github.com/[YOUR REPO PATH]",
  "bugs": {
    "url": "https://github.com/[YOUR REPO PATH]/issues"
  },
  "keywords": [],
  "scripts": {
    "boot": "node build/index",
    "boot.local": "node -r ts-node/register -r tsconfig-paths/register ./src/index",
    "lint": "eslint '**/*.ts' && yarn lint.tsc",
    "lint.tsc": "tsc --noEmit && echo 'PASS'",
    "lint.fix": "eslint --fix '**/*.ts'",
    "lint.watch": "chokidar 'src/**/*' '../spruce-templates/src/**' -c 'yarn lint.tsc'",
    "health": "yarn run boot --health",
    "health.local": "yarn run boot.local --health",
    "build": "yarn build.babel && yarn build.resolve-paths",
    "build.types": "tsc --emitDeclarationOnly",
    "build.babel": "babel src --out-dir build --extensions '.ts, .tsx' --source-maps --copy-files",
    "build.watch": "babel src --out-dir build --extensions '.ts, .tsx' --source-maps --copy-files --watch",
    "build.resolve-paths": "resolve-path-aliases build && resolve-path-aliases node_modules/@sprucelabs/schema/build",
    "clean": "rm -rf build/",
    "clean.all": "rm -rf build/ && rm -rf node_modules/"
  },
  "dependencies": {
    "@sprucelabs/error": "^2.2.186",
    "@sprucelabs/log": "^10.2.186",
    "@sprucelabs/schema": "^9.0.11",
    "@sprucelabs/spruce-skill-utils": "^10.25.2",
    "globby": "^11.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@sprucelabs/babel-plugin-schema": "^1.2.1",
    "@types/node": "^14.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.8.1",
    "eslint-config-spruce": "^10.9.30",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}